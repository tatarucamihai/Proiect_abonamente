<Shell
    x:Class="Proiect_abonamente.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proiect_abonamente"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>

        <ShellContent
        Title="Register"
        Route="Register"
        ContentTemplate="{DataTemplate local:SignUpPage}"
        />

        <ShellContent
        Title="Login"
        Route="Login"
        ContentTemplate="{DataTemplate local:HomePage}"
         />


        <ShellContent
        Title="Logout"
        Route="logout"
        ContentTemplate="{DataTemplate local:LogoutPage}" />

        <ShellContent
        Title="About"
        ContentTemplate="{DataTemplate local:AboutPage}"
        Route="About"
/>



        <ShellContent
        Title="My Subscriptions"
        ContentTemplate="{DataTemplate local:SubscribtionEntryPage}"/>
    </TabBar>

</Shell>
